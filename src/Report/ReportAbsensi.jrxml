	<queryString>
		<![CDATA[SELECT
    k.nip,
    k.nama,
    a.tanggal,
    a.waktu_masuk,
    a.waktu_keluar,
    a.status
FROM
    absensi a
LEFT JOIN karyawan k ON
    k.id = a.karyawan_id
WHERE
a.STATUS
    = 'Hadir'
ORDER BY
    a.id
DESC]]>
